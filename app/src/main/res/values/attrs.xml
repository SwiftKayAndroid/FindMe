<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="SwipeFlingStyle" format="reference"/>
        <attr name="rotation_degrees" format="float"/>
        <attr name="min_adapter_stack" format="integer"/>
        <attr name="max_visible" format="integer"/>
    </declare-styleable>

    <declare-styleable name="Emojicon">
        <attr name="emojiconSize" format="dimension"/>
        <attr name="emojiconTextStart" format="integer"/>
        <attr name="emojiconTextLength" format="integer"/>
        <attr name="emojiconUseSystemDefault" format="boolean"/>
    </declare-styleable>

    <!--this is used for floating action button-->
    <attr name="icActionEdit" format="reference"/>

    <!-- Default style attribute which will be used during construction of FloatingActionButton. -->
    <attr name="floatingActionButtonStyle" format="reference"/>



    <declare-styleable name="StaggeredGridView">
        <attr name="drawSelectorOnTop" format="boolean" />
        <attr name="numColumns" format="integer" />
        <attr name="itemMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TabHolder">
        <attr name="numberOfTabs" format="integer"/>
    </declare-styleable>

</resources>